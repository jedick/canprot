Package: canprot
Version: 1.1.1-7
Title: Chemical and Thermodynamic Analysis of Protein Expression
Authors@R: c(
    person("Jeffrey", "Dick", email = "j3ffdick@gmail.com", role = c("aut", "cre"),
      comment = c(ORCID = "0000-0002-0687-5890")),
